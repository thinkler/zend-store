{% if (isAllowed('userData')) %}
    <h3 class="text-center">{{order.user.email}}</h3>
{% endif %}

<div class="thumbnail">
    <table class="table table-hover">
        <tr>
            <th>Title</th>
            <th>Price</th>
        </tr>
        {% for item in items %}
            <tr>
                <td>{{ item.title }}</td>
                <td>{{ item.price }}</td>
            </tr>
        {% endfor %}
    </table>
</div>
